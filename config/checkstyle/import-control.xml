<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC "-//Puppy Crawl//DTD Import Control 1.1//EN" "http://www.puppycrawl.com/dtds/import_control_1_1.dtd">
<import-control pkg="jp.xet.springconfig.aws">
  
  <!-- 明示的禁止以外は全て許可 -->
  <allow class=".*" regex="true" />
  
  <!-- これに引っかかるのは多分インポートミス -->
  <disallow pkg="javax.annotation"/>
  
  <disallow pkg="com.amazonaws.services"/>
  <disallow pkg="software.amazon.awssdk.services"/>
</import-control>
